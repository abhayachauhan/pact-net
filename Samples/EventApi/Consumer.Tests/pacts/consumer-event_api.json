{
  "provider": {
    "name": "Event API"
  },
  "consumer": {
    "name": "Consumer"
  },
  "interactions": [
    {
      "description": "a request to check the api status",
      "request": {
        "method": "get",
        "path": "/stats/status",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "alive": true,
          "_links": {
            "uptime": {
              "href": "/stats/uptime"
            }
          }
        }
      },
      "responseMatchingRules": {
        "body": {
          "$._links.uptime.href": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecificationVersion": "1.1.0"
  }
}